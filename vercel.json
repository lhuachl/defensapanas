{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "cleanUrls": false,
  "trailingSlash": false,
  "functions": {
    "app/**": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/capitulo2",
      "destination": "/capitulo2.html"
    },
    {
      "source": "/capitulo4", 
      "destination": "/capitulo4.html"
    }
  ]
}
